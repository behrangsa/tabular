{
  "manifest_version": 3,
  "name": "Tabular",
  "version": "1.0.0",
  "description": "A Chrome extension that adds browser action functionality",
  "action": {
    "default_title": "Tabular",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": []
}
